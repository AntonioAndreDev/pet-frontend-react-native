    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.21",
    "expo-status-bar": "~2.0.1",
    "lodash": "^4.17.21",
    "nativewind": "latest",
    "react": "18.3.1",
